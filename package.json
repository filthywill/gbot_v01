{
  "name": "gbot_v01",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/filthywill/gbot_v01"
  },
  "npmConfig": {
    "legacy-peer-deps": true
  },
  "scripts": {
    "dev": "vite --port 3000 --host",
    "dev:https": "cross-env VITE_USE_HTTPS=true vite --port 3000 --host",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "preview:https": "cross-env VITE_USE_HTTPS=true vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "analyze": "cross-env ANALYZE=true vite build",
    "analyze:dev": "cross-env ANALYZE=true vite build --mode development",
    "analyze:size": "npm run build && ls -lh dist/assets/*.js && echo \"Total bundle size:\" && du -sh dist/assets/*.js | awk '{sum += $1} END {print sum \"MB\"}'",
    "bundle:report": "npm run analyze && echo \"Bundle analysis complete! Check dist/bundle-analysis.html\"",
    "type-check": "tsc --noEmit --incremental",
    "type-check:watch": "tsc --noEmit --incremental --watch",
    "✅ PHASE 3: Advanced TypeScript Workflows": "// ⬇️ NEW SCRIPTS ⬇️",
    "ts:check": "npm run type-check",
    "ts:check:verbose": "tsc --noEmit --incremental --listFiles",
    "ts:check:performance": "tsc --noEmit --incremental --diagnostics --extendedDiagnostics",
    "ts:check:strict": "tsc --noEmit --incremental --strict --noImplicitAny --strictNullChecks",
    "ts:analyze": "tsc --noEmit --incremental --generateTrace .tsbuildinfo/trace",
    "ts:clean": "rimraf .tsbuildinfo && echo \"TypeScript build cache cleared\"",
    "ts:reset": "npm run ts:clean && npm run ts:check",
    "ts:benchmark": "node scripts/ts-benchmark.js",
    "ts:monitor": "node scripts/ts-monitor.js check",
    "ts:monitor:full": "node scripts/ts-monitor.js full",
    "ts:monitor:report": "node scripts/ts-monitor.js report",
    "quality:check": "npm run lint && npm run type-check && npm run test:run",
    "quality:fix": "eslint . --ext ts,tsx --fix && npm run type-check",
    "quality:watch": "npm run type-check:watch & npm run test:ui",
    "ci:type-check": "npm run ts:check",
    "ci:build": "npm run quality:check && npm run build",
    "ci:test": "npm run ts:check && npm run test:run"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.49.4",
    "@types/zxcvbn": "^4.4.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "html2canvas": "^1.4.1",
    "lucide-react": "^0.344.0",
    "react": "^19.1.0",
    "react-colorful": "^5.6.1",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.1",
    "react-router-dom": "^7.5.1",
    "simplify-path": "^1.1.0",
    "svgpath": "^2.6.0",
    "tailwind-merge": "^3.2.0",
    "zustand": "^4.5.7",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/svgo": "^2.6.4",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/ui": "^3.1.4",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.35",
    "rimraf": "^6.0.1",
    "rollup-plugin-visualizer": "^6.0.1",
    "svgo": "^3.3.2",
    "tailwind-scrollbar": "^4.0.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^6.2.2",
    "vitest": "^3.1.4"
  }
}
